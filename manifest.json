{
  "name": "Image Style Editor",
  "description": "Changing image style lively",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "storage"
  ],"content_scripts": [
    {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["js/content.js"]
    }
],"background": {
    "scripts": ["js/background.js"],
    "persistent": false
},
  "manifest_version": 2
}